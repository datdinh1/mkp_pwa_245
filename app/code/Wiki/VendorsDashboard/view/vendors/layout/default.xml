<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendor-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Wiki_VendorsDashboard::css/wysiwyg-responsive.css" />
        <css src="Wiki_VendorsDashboard::css/bootstrap.min.css" />
        <css src="Wiki_VendorsDashboard::css/font-awesome.min.css" />
        <css src="Wiki_VendorsDashboard::css/ionicons.min.css" />
        <css src="Wiki_VendorsDashboard::css/AdminLTE.css" />
        <css src="Wiki_VendorsDashboard::css/skins/_all-skins.min.css" />
        <css src="Wiki_VendorsDashboard::css/custom.css" />
        <link src_type="url" type="text/css" src='https://fonts.googleapis.com/css?family=Sarabun:ital,wght@0,300;0,400;0,600;0,700;1,300;1,400;1,600;1,700&amp;display=swap' rel='stylesheet' />
        <link src_type="url" type="text/css" src="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"/>
        <css src="Wiki_VendorsDashboard::css/custom_lib.css" />
    </head>
    <body>
    	<attribute name="class" value="hold-transition"/>
    	<attribute name="class" value="sidebar-mini"/>
    	<referenceContainer name="page.wrapper" htmlClass="wrapper"/>
    	<referenceContainer name="header" htmlClass="main-header">
    	</referenceContainer>
		<referenceContainer name="page.content" htmlClass="page-content content-wrapper" />
		<referenceContainer name="page.main.container" htmlClass="content" htmlTag="section" />
		<referenceContainer name="footer" htmlClass="main-footer" />
		<move element="messages.wrapper" before="-" destination="page.main.container" />

    </body>
</page>
