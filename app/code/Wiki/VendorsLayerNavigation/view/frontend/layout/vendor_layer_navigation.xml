<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="div.sidebar.additional">
            <block class="Wiki\VendorsLayerNavigation\Block\Navigation\Category" name="vendor.catalog.leftnav" after="vendor.profile.left" template="Wiki_VendorsLayerNavigation::layer/view.phtml">
