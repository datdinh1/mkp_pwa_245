<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
        <css src="Wiki_VendorsCoupon::css/coupon.css" />
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Wiki\VendorsCoupon\Block\Vendors\Coupon" name="seller.coupon" template="coupon.phtml">
            	<arguments>
	    			<argument name="block_title" xsi:type="string">Inbox</argument>
    			</arguments>
            	<block class="Wiki\VendorsCoupon\Block\Vendors\Coupon\Form" name="seller.coupon.generate_form" as="generate_form" template="coupon/form.phtml">

            	</block>
            	<uiComponent name="vendor_coupon_grid" as="coupon_grid"/>
            </block>
        </referenceContainer>
    </body>
</page>
