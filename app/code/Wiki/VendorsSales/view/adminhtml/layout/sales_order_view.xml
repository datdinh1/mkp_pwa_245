<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Wiki_VendorsSales::css/custom.css"/>
	</head>
    <body>
    	<referenceBlock name="order_items">
			<block class="Wiki\VendorsSales\Block\Adminhtml\Order\View" name="vendors_update_order_button"/>
    		<action method="setTemplate">
		        <argument name="template" xsi:type="string">Wiki_VendorsSales::order/view/items.phtml</argument>
		    </action>
    	</referenceBlock>
		<referenceBlock name="order_history">
			<action method="setTemplate">
		        <argument name="template" xsi:type="string">Wiki_VendorsSales::order/view/history.phtml</argument>
		    </action>
		</referenceBlock>
    </body>
</page>
